{
  "name": "bamboo-cli",
  "description": "command-line tool to interact with Atlassian Bamboo",
  "version": "0.0.0",
  "author": "jmbeach",
  "bin": {
    "bamboo-cli": "./bin/run"
  },
  "bugs": "https://github.com/jmbeach/bamboo-cli/issues",
  "dependencies": {
    "@oclif/color": "^0.1.0",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@types/supports-color": "^5.3.0",
    "axios": "^0.19.1",
    "conf": "^6.2.0",
    "conf-cli": "^0.1.9",
    "json-stringify-safe": "^5.0.1",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1.2.5",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.2.7",
    "@types/faker": "^4.1.9",
    "@types/json-stringify-safe": "^5.0.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10",
    "@types/proxyquire": "^1.3.28",
    "chai": "^4.2.0",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "faker": "^4.1.0",
    "globby": "^10",
    "mocha": "^7.0.0",
    "nock": "^11.7.2",
    "nyc": "^15.0.0",
    "proxyquire": "^2.1.3",
    "rimraf": "^3.0.0",
    "sinon": "^8.1.0",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/jmbeach/bamboo-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "bamboo-cli",
    "plugins": [
      "@oclif/plugin-help",
      "conf-cli"
    ],
    "hooks": {
      "init": "./lib/hooks/init/init"
    },
    "topics": {
      "build": {
        "description": "commands for getting build results/status"
      }
    }
  },
  "repository": "jmbeach/bamboo-cli",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "test:watch": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\" --watch",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
